        <!-- Header -->

        <header class="header clearfix">

            <a href="/" class="header_logo">
                Homing <span>in</span>
            </a>

            <h1 class="header_title">What's my home worth?</h1>

            <nav id="main-header" class="header_nav clearfix">
                <id ng-if="currentUser == null || currentUser.FirstName == 'Anonymous' && currentUser.LastName =='Anonymous'">
                    <a href="/homeowners" class="header_nav-item">Homeowners</a>
                    <a href="/pricing" class="header_nav-item">Agents</a>
                    <a href="http://blog.homingin.co" class="header_nav-item">Blog</a>
                    @*<a href="/promo" class="header_nav-item">Promo</a>*@
                </id>
                
                <id ng-if="currentUser.IsAgent && !currentUser.IsPremium">
                    <a href="/pricing" class="header_nav-item">UPGRADE TO PREMIUM NOW</a>
                    <a href="http://blog.homingin.co" class="header_nav-item">Blog</a>
                </id>

                <id ng-if="currentUser.IsAgent && currentUser.IsPremium">
                    <a href="http://blog.homingin.co" class="header_nav-item">Blog</a>
                </id>
                  
                    <id ng-if="currentUser == null || currentUser.FirstName == 'Anonymous' && currentUser.LastName =='Anonymous'">
                        <a href="/login" class="header_nav-item login-link"><span class="pe-7s-unlock"></span> Login</a>
                    </id>


                    <id ng-if="currentUser != null && currentUser.FirstName != 'Anonymous' && currentUser.LastName != 'Anonymous'">
                        <a href="/dashboard/properties" class="header_nav-item header_user">
                            <div class="header_user-avatar">
                                <span user-pic="{{currentUser.Picture.URL}}"></span>
                            </div>
                            <span class="header_user-name">{{currentUser.FirstName}} {{currentUser.LastName}}</span>
                        </a>
                    </id>


                    <a href="{{AndroidURL}}" class="header_nav-item get-app-link" ng-if="IsAndroid">Get the app</a>
                    <a href="{{IOSUrl}}" class="header_nav-item get-app-link" ng-if="IsIOS">Get the app</a>

</nav>

            <id ng-if="currentUser != null && currentUser.FirstName != 'Anonymous' && currentUser.LastName != 'Anonymous'">
                <a href="/dashboard/properties" class="header_user header_user-mobile">
                    <div class="header_user-avatar">
                        <span user-pic="{{currentUser.Picture.URL}}"></span>
                    </div>
                    <span class="header_user-name">{{currentUser.FirstName}} {{currentUser.LastName}}</span>
                </a>
            </id>

            <div class="mobile-tgl" data-toggle="dropdown" data-target="#main-header">
                <span></span>
            </div>

        </header>

        <!-- END Header -->

