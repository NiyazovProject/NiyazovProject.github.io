        <!-- Last step popup -->
        <div class="popup-body submit-review-popup" style="display: none;" id="genericMessageBoxPopup">

            <div class="popup-window">
                <span class="pe-7s-close-circle close-popup" ng-click="HideGenericMessageBox()"></span>
                <h3 class="popup_title" id="genericMessageBoxPopupTitle">Error!</h3>

                <p class="popup_caption" id="genericMessageBoxPopupMessage">Something happened!</p>

                <button class="btn" ng-click="HideGenericMessageBox()">Ok</button>

            </div>

        </div>
    
        <div class="popup-body submit-review-popup" style="display: none;" id="genericConfirmBoxPopup">

            <div class="popup-window">
                <span class="pe-7s-close-circle close-popup js-close-popup"></span>

                <h3 class="popup_title" id="genericConfirmBoxPopupTitle">Error!</h3>
                <p class="popup_caption" id="genericConfirmBoxPopupMessage">Something happened!</p>

                <button class="btn btn-text js-close-popup" id="genericConfirmBoxPopupCancel">Cancel</button>
                <button class="btn btn-danger js-close-popup" id="genericConfirmBoxPopupConfirm">Confirm</button>

            </div>

        </div>
